<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="1000.0" prefWidth="1000.0" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.cms.canteenmanagementsystem.AdminMenu">
   <children>
      <AnchorPane prefHeight="1000.0" prefWidth="1000.0">
         <children>
            <ImageView fx:id="AdminImageView" fitHeight="1000.0" fitWidth="1000.0" layoutX="-1.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../images/full-page-background.PNG" />
               </image>
            </ImageView>
            <Label alignment="CENTER" layoutX="-3.0" layoutY="-1.0" prefHeight="142.0" prefWidth="1000.0" style="-fx-background-color: black;" text="ADMIN MENU" textFill="#f7f1f1">
               <font>
                  <Font size="96.0" />
               </font>
            </Label>
            <Button fx:id="FoodMenuBtn" layoutX="260.0" layoutY="150.0" mnemonicParsing="false" onAction="#FoodMenuBtn" prefHeight="111.0" prefWidth="598.0" text="Food Profile">
               <font>
                  <Font size="70.0" />
               </font></Button>
            <Button fx:id="CustomerMenuBtn" layoutX="262.0" layoutY="300.0" mnemonicParsing="false" onAction="#CustomerMenuBtn" prefHeight="111.0" prefWidth="598.0" text="Customer Profile">
               <font>
                  <Font size="67.0" />
               </font></Button>
            <Button fx:id="BackBtn" layoutX="702.0" layoutY="650.0" mnemonicParsing="false" onAction="#BackBtn" prefHeight="118.0" prefWidth="151.0" style="-fx-background-color: blue;" text="Back" textFill="#ece4e4">
               <font>
                  <Font size="38.0" />
               </font></Button>
            <Button fx:id="CloseBtn" layoutX="108.0" layoutY="652.0" mnemonicParsing="false" onAction="#CloseBtn" prefHeight="118.0" prefWidth="174.0" style="-fx-background-color: red;" text="Close" textFill="#faf5f5">
               <font>
                  <Font size="44.0" />
               </font></Button>
            <Button layoutX="262.0" layoutY="444.0" mnemonicParsing="false" onAction="#EmployeeMenuBtn" prefHeight="117.0" prefWidth="598.0" text="Employee Profile">
               <font>
                  <Font size="53.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
